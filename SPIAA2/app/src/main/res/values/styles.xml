<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#5F5E5E</item>
        <item name="colorPrimaryDark">#474747</item>
        <item name="colorAccent">#E5194C</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="AppTheme.NoActionBar" parent="Base.AppTheme.NoActionBar" />


    <style name="Base.AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#5F5E5E</item>
        <item name="colorPrimaryDark">#474747</item>
        <item name="colorAccent">#E5194C</item>
    </style>

    <style name="AppTheme.Button.Primary" parent="Base.Widget.AppCompat.Button">

        <item name="colorButtonNormal">@color/red_padrao</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

</resources>
